<Window x:Class="Farmapoint.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Farmapoint"
        mc:Ignorable="d"
        WindowState="Maximized"
        ResizeMode="NoResize"
        WindowStartupLocation="CenterScreen"
        Title="Principal"
        Width="1000"
        Icon="Resources/icono.ico" 
        WindowStyle="None">
    <Window.Background>
        <LinearGradientBrush  EndPoint="0.5,1"  >
            <GradientStop Color="#FF2DA622" Offset="0"/>
            <GradientStop Color="#FF3E7038" Offset="0.461"/>
            <GradientStop Color="#FF5C9355" Offset="0.61"/>
            <GradientStop Color="#FF526150" Offset="0.803"/>
        </LinearGradientBrush>
    </Window.Background>
    <Frame Source="PageMainWindow.xaml" NavigationUIVisibility="Hidden" />
</Window>
