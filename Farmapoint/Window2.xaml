<Window x:Class="Farmapoint.Window2"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Farmapoint"
        mc:Ignorable="d"
        Title="Window2"
        WindowState="Maximized"
        Background="WhiteSmoke">
    <Grid>
        <Label x:Name="Title" Content="Farmapoint" HorizontalAlignment="Center" VerticalAlignment="Top" Height="64" Width="auto" FontSize="36" FontWeight="Bold" FontStyle="Italic" Margin="278,43,277.6,0" />
        <Button Content="Volver" HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="25,0,0,25" Width="auto" Height="auto" Click="Button_Volver"/>
        <DataGrid x:Name="grdDatos" HorizontalAlignment="Center" Width="auto" MinWidth="500" MaxHeight="300" MinHeight="250" VerticalAlignment="Center" Loaded="DataRow_Loaded" IsReadOnly="True"/>
    </Grid>
</Window>
